{
  "name": "Paranoid ID Manager",
  "description": "ID manager helper for Paranoid",
  "version": "1.0",
  "manifest_version": 2,
  "incognito":"split",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["placeholder.js"]
    }
  ],
  "browser_action": {
    "default_title": "Paranoid"
  },
  "permissions": ["storage", "activeTab"]
}
